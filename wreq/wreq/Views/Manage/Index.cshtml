@model wreq.Models.IndexViewModel
@{
    ViewBag.Title = Resource.ManageAccount;
}

<h2>@ViewBag.Title.</h2>

<p class="text-success">@ViewBag.StatusMessage</p>
<div>
    <h4>@Resource.ChangeAccountSettings</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Password:</dt>
        <dd>
            @Html.ActionLink(Resource.ChangePassword, "ChangePassword")
        </dd>
    </dl>
</div>
